create table userBos (
userId int auto_increment,
userName varchar(50),
userEmail varchar(50) unique,
userPassword varchar(256),
registerDate date,
primary key (userId)
);

create table administrator (
userId int auto_increment,
userName varchar(50),
userEmail varchar(50) unique,
userPassword varchar(256),
registerDate date,
primary key (userId)
);

create table workSheet (
workSheetId int auto_increment,
workPlace varchar(256),
userId int unique,
workDate varchar(20) unique,
timeIn varchar(10),
timeOut varchar(10),
WorksheetState int,
observations text,
primary key(workSheetId),
foreign key (userId) references userBos(userId)
);

create table workSheetHistorical (
workSheetHistoricalId int auto_increment,
workSheetId int,
primary key (workSheetHistoricalId),
foreign key (workSheetId) references workSheet(workSheetId)
);
